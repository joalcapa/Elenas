{
  "description": "Componente que permite llenar la informacion de la direccion del cliente.",
  "displayName": "AddressForm",
  "methods": [],
  "props": {
    "city": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "Ciudad del cliente.",
      "defaultValue": {
        "value": "''",
        "computed": false
      }
    },
    "country": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "Pais del cliente.",
      "defaultValue": {
        "value": "''",
        "computed": false
      }
    },
    "stateName": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "Nombre del departamento del cliente.",
      "defaultValue": {
        "value": "''",
        "computed": false
      }
    },
    "streetAddress": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "Direccion del cliente.",
      "defaultValue": {
        "value": "''",
        "computed": false
      }
    },
    "statesForm": {
      "type": {
        "name": "array"
      },
      "required": false,
      "description": "Array de los departamentos del formulario.",
      "defaultValue": {
        "value": "[]",
        "computed": false
      }
    },
    "citiesForm": {
      "type": {
        "name": "array"
      },
      "required": false,
      "description": "Array de las ciudades del departamento seleccionado del formulario.",
      "defaultValue": {
        "value": "[]",
        "computed": false
      }
    },
    "onSelectStateForm": {
      "type": {
        "name": "func"
      },
      "required": false,
      "description": "Funcion de seleccion del departamento.",
      "defaultValue": {
        "value": "() => {}",
        "computed": false
      }
    },
    "onSelectCityForm": {
      "type": {
        "name": "func"
      },
      "required": false,
      "description": "Funcion de seleccion de la ciudad del departamento.",
      "defaultValue": {
        "value": "() => {}",
        "computed": false
      }
    },
    "changeStreetAddress": {
      "type": {
        "name": "func"
      },
      "required": false,
      "description": "Funcion que cambio el valor de la direccion.",
      "defaultValue": {
        "value": "() => {}",
        "computed": false
      }
    },
    "changeCountry": {
      "type": {
        "name": "func"
      },
      "required": false,
      "description": "Funcion que cambia el valor del pais.",
      "defaultValue": {
        "value": "() => {}",
        "computed": false
      }
    },
    "createCustomer": {
      "type": {
        "name": "func"
      },
      "required": false,
      "description": "Funcion que crea al cliente.",
      "defaultValue": {
        "value": "() => {}",
        "computed": false
      }
    },
    "isValidAddressForm": {
      "type": {
        "name": "bool"
      },
      "required": false,
      "description": "Identifica si el formulario de la direccion es valido.",
      "defaultValue": {
        "value": "false",
        "computed": false
      }
    },
    "isEdit": {
      "type": {
        "name": "bool"
      },
      "required": false,
      "description": "Identifica si se edita al cliente.",
      "defaultValue": {
        "value": "false",
        "computed": false
      }
    },
    "onBack": {
      "type": {
        "name": "func"
      },
      "required": false,
      "description": "Funcion que permite volver al formulario del cliente.",
      "defaultValue": {
        "value": "() => {}",
        "computed": false
      }
    }
  }
}
